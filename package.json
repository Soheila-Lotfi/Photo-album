    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "multer": "^1.4.2",
    "aws-sdk": "2.x",
    "ejs": "2.x"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "nodemon": "^1.19.4"
  }
